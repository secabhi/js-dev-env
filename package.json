{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "JAVASCRIPT development Environment for developers",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMsg.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "hosturl": "lt --port 3001 --subdomain abhishek",
    "share-view": "npm-run-all --parallel open:src hosturl",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3002",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secabhi/js-dev-env.git"
  },
  "author": "Abhishek sinha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/secabhi/js-dev-env/issues"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "cross-env": "3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "^0.13.1",
    "surge": "^0.18.0",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5",
    "whatwg-fetch": "^2.0.1"
  },
  "homepage": "https://github.com/secabhi/js-dev-env#readme"
}
